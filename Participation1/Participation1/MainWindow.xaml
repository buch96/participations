<Window x:Class="Participation1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Participation1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Height="23" Margin="146,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FFD8503A"/>
        <TextBox x:Name="AddressBox" HorizontalAlignment="Left" Height="23" Margin="146,146,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FF459B26"/>
        <TextBox x:Name="ZipBox" HorizontalAlignment="Left" Height="23" Margin="146,203,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FF8A91E0"/>
        <Button x:Name="EnterFormButton" Content="Enter" HorizontalAlignment="Left" Margin="580,10,0,0" VerticalAlignment="Top" Width="202" Click="EnterFormButton_Click" Height="166" Background="Magenta" FontSize="48" FontFamily="Rosewood Std Regular" FontWeight="Bold"/>
        <ListBox x:Name="BigYellowListBox" HorizontalAlignment="Left" Height="206" Margin="303,203,0,0" VerticalAlignment="Top" Width="479" Background="#FFE8E88F"/>

    </Grid>
</Window>
